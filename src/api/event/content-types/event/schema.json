{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Events",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "standard"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "date_start": {
      "type": "date"
    },
    "date_end": {
      "type": "date"
    },
    "archive": {
      "type": "boolean",
      "default": false
    },
    "areas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::area.area"
    },
    "slug": {
      "type": "uid",
      "maxLength": 50,
      "required": true,
      "targetField": "title"
    },
    "attached_files": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "picture": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
