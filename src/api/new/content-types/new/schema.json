{
  "kind": "collectionType",
  "collectionName": "news",
  "info": {
    "singularName": "new",
    "pluralName": "news",
    "displayName": "News ",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "output": "HTML",
        "preset": "defaultHtml"
      }
    },
    "picture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "attached_files": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "archive": {
      "type": "boolean",
      "default": false
    },
    "areas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::area.area"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true,
      "maxLength": 50
    },
    "video": {
      "type": "customField",
      "customField": "plugin::video-field.video"
    }
  }
}
